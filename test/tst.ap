macro hamming_distance (String s, int temp) {
    int d = temp + -1;
    Counter cnt;
    foreach (char c : s) {
        if(c != input())
            cnt.count();
    }
    if(cnt <= d)
        report;
    cnt.reset();
}

macro test (char d) {
    if(!(d == input() && d == input()))
        report;
}

network (String nodes[], int d) {
    hamming_distance("hello", 4);
    hamming_distance("jello", 4);
}